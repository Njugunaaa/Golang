package main

import "fmt"

func main(){


var publisher string
var writer string
var artist string
var title string
var year int
var pageNumber int
var grade float32
publisher = "DizzyBooks Publishing Inc." 
writer = "Tracey Hatchet"
artist = "Jewel Tampson"
title = "Mr. GoToSleep"
year = 1997
pageNumber = 14
grade = 6.5

const  price =  100 
totalPrice := price * pageNumber 

fmt.Println(title, "written by", writer, "drawn by", artist, "published by", publisher,"in the year", year, "with", pageNumber, "pages and", grade, "grade","is of ", totalPrice, "price" )

publisher = "DizzyBooks Publishing Inc."
writer = "Ryan N. Shawn"
artist = "Phoebe Paperclips"
title = "Epic Vol. 1"
year = 2013
pageNumber = 160
grade = 9.0



fmt.Println(title, "written by", writer, "drawn by", artist, "published by", publisher,"in the year", year, "with", pageNumber, "pages and", grade, "grade","is of ", totalPrice, "price" )

}
